{
  "title": "Exam Kubernetes - Backend Metrics",
  "tags": ["kubernetes", "prometheus", "backend"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Total Requêtes (COUNT)",
      "type": "stat",
      "gridPos": {
        "h": 8,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(increase(http_requests_total{namespace=\"todolist\",route!~\"(/metrics${exclude_health_ready})\"}[$__range]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "none",
        "colorMode": "value",
        "justifyMode": "auto",
        "orientation": "auto"
      }
    },
    {
      "id": 2,
      "title": "RPS par méthode HTTP",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 9,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (method) (rate(http_requests_total{namespace=\"todolist\",route!~\"(/metrics${exclude_health_ready})\"}[5m]))",
          "refId": "A",
          "legendFormat": "{{method}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "decimals": 2,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "mappings": []
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "/DELETE/"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 3,
      "title": "Erreurs 500 dans le temps",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 9,
        "x": 15,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum by (route, method) (increase(http_requests_total{namespace=\"todolist\",route!~\"(/metrics${exclude_health_ready})\",status=\"500\"}[1m]))",
          "refId": "A",
          "legendFormat": "{{method}} {{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 30,
            "gradientMode": "opacity",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "always",
            "pointSize": 5,
            "stacking": {
              "group": "A",
              "mode": "normal"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "line"
            }
          },
          "color": {
            "mode": "fixed",
            "fixedColor": "red"
          },
          "mappings": []
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["last", "max", "sum"]
        }
      }
    },
    {
      "id": 4,
      "title": "Latence p95",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95,\n  sum(rate(http_request_duration_seconds_bucket{namespace=\"todolist\",route!~\"(/metrics${exclude_health_ready})\"}[5m])) by (le, route)\n)",
          "refId": "A",
          "legendFormat": "{{route}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 3,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 1,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "mappings": []
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      }
    },
    {
      "id": 5,
      "title": "Routes et méthodes HTTP",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "expr": "sum by (route, method) (increase(http_requests_total{namespace=\"todolist\",route!~\"(/metrics${exclude_health_ready})\"}[$__range]))",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "displayMode": "auto",
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "route"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 300
              },
              {
                "id": "displayName",
                "value": "Route"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "method"
            },
            "properties": [
              {
                "id": "custom.width",
                "value": 100
              },
              {
                "id": "displayName",
                "value": "Méthode"
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-text"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "options": {
                      "DELETE": {
                        "color": "red",
                        "text": "DELETE"
                      },
                      "GET": {
                        "color": "green",
                        "text": "GET"
                      },
                      "POST": {
                        "color": "blue",
                        "text": "POST"
                      },
                      "PUT": {
                        "color": "orange",
                        "text": "PUT"
                      },
                      "PATCH": {
                        "color": "purple",
                        "text": "PATCH"
                      }
                    },
                    "id": "value"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Value"
            },
            "properties": [
              {
                "id": "unit",
                "value": "short"
              },
              {
                "id": "decimals",
                "value": 0
              },
              {
                "id": "displayName",
                "value": "COUNT"
              },
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              },
              {
                "id": "thresholds",
                "value": {
                  "mode": "absolute",
                  "steps": [
                    {
                      "value": null,
                      "color": "green"
                    },
                    {
                      "value": 1,
                      "color": "yellow"
                    },
                    {
                      "value": 5,
                      "color": "red"
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "RPS"
          }
        ]
      }
    },
    {
      "id": 7,
      "title": "Nombre de requêtes dans le temps",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 18,
        "x": 0,
        "y": 16
      },
      "targets": [
        {
          "expr": "sum by (method, route, status) (increase(http_requests_total{namespace=\"todolist\",route!~\"(/metrics${exclude_health_ready})\"}[1m]))",
          "refId": "A",
          "legendFormat": "{{method}} {{route}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "barAlignment": 0,
            "lineWidth": 2,
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "auto",
            "pointSize": 5,
            "stacking": {
              "group": "A",
              "mode": "none"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "tooltip": false,
              "viz": false,
              "legend": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          },
          "color": {
            "mode": "palette-classic"
          },
          "mappings": []
        },
        "overrides": [
          {
            "matcher": {
              "id": "byRegexp",
              "options": "/5../"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "/2../"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "/4../"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "yellow"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "showLegend": true,
          "calcs": ["last", "max"]
        }
      }
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "timepicker": {},
  "templating": {
    "list": [
      {
        "name": "exclude_health_ready",
        "type": "custom",
        "label": "Exclure /health et /ready",
        "description": "Oui = exclure /health et /ready des métriques, Non = les inclure",
        "query": "Oui:|/health|/ready,Non:",
        "current": {
          "value": "|/health|/ready",
          "text": "Oui"
        },
        "options": [
          {
            "value": "|/health|/ready",
            "text": "Oui"
          },
          {
            "value": "",
            "text": "Non"
          }
        ],
        "hide": 0
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "weekStart": ""
}
